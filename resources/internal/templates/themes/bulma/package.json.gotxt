{
	"name": "{{ .ProjectName }}",
	"version": "0.1.0",
	"packageManager": "{{ .NPMClient }}",
	"scripts": {
		"dev": "svelte-kit dev --port {{ .PortNumber }}",
		"clean": "rimraf build",
		"build": "npm run clean && svelte-kit build",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@indaco/svelte-iconoir": "^2.3.0",
		"@sveltejs/adapter-static": "1.0.0-next.29",
		"@sveltejs/kit": "1.0.0-next.302",
		"@sveltinio/essentials": "^0.1.2",
		"@sveltinio/seo": "^0.1.5",
		"@sveltinio/services": "^0.2.0",
		"@sveltinio/widgets": "^0.1.5",
		"@types/gtag.js": "^0.0.8",
		"@types/lodash-es": "^4.17.5",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"bulma": "^0.9.3",
		"dayjs": "^1.10.7",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"install": "^0.13.0",
		"lodash-es": "^4.17.21",
		"mdast-util-to-string": "^3.1.0",
		"mdsvex": "^0.10.5",
		"mdsvex-relative-images": "^1.0.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-slug": "^5.0.1",
		"remark-emoji": "^3.0.2",
		"remark-external-links": "9.0.1",
		"remark-preview": "^1.0.2",
		"remark-reading-time": "1.0.1",
		"remark-slug": "^7.0.1",
		"rimraf": "^3.0.2",
		"sass": "^1.46.0",
		"svelte": "^3.46.3",
		"svelte-check": "^2.3.0",
		"svelte-preprocess": "^4.10.2",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5",
		"unist-util-visit": "^4.1.0"
	},
	"type": "module"
}
